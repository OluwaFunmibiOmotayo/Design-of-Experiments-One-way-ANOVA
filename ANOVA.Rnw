% Step 5: Running One-Way ANOVA
\begin{frame}[fragile]
    \frametitle{Step 5: Running One-Way ANOVA}
    
    \textbf{ANOVA Model}

    \begin{minted}[fontsize=\small]{r}
    # Perform ANOVA
    anova_mod <- aov(Response ~ Treatment, data = dat_CRD_long)
    summary(anova_mod)
    \end{minted}

