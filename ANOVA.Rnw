\begin{frame}[fragile]
\frametitle{Step 5: Assumption Checks for ANOVA}
\begin{columns}
\column{0.5\textwidth}
\textbf{Residual Analysis}
\begin{minted}[fontsize=\tiny]{r}
# Plot residuals
plot(anova_mod)
\end{minted}
